        <activity
            android:name=".ColoursCategoryActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".DaysCategoryActivity" />
        <activity
            android:name=".FoodCategoryActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AnimalsCategoryActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SchoolCategoryActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TransportationCategoryActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BodyPartsCategoryActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EasyQuiz"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResultActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AlphabetsCategoryActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CategoryActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".selectquiz"></activity>
    </application>

</manifest>